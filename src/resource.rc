#include <windows.h>
#include "../include/OmicronTK/Qt/version.h"

1 VERSIONINFO
 FILEVERSION OTKQT_PRODUCT_VERSION
 PRODUCTVERSION OTKQT_PRODUCT_VERSION
 FILEFLAGSMASK 0x3fL
#ifdef _DEBUG
 FILEFLAGS 0x1L
#else
 FILEFLAGS 0x0L
#endif
 FILEOS VOS__WINDOWS32
 FILETYPE VFT_DLL
 FILESUBTYPE 0x0L
BEGIN
    BLOCK "StringFileInfo"
    BEGIN
        BLOCK "040904b0"
        BEGIN
            VALUE "CompanyName",       "Fábio Pichler"
            VALUE "FileDescription",   "Omicron ToolKit + Qt"
            VALUE "FileVersion",       OTKQT_FILE_VERSION
            VALUE "InternalName",      ORIGINAL_FILENAME
            VALUE "LegalCopyright",    "Copyright © 2018-2019, Fábio Pichler"
            VALUE "OriginalFilename",  ORIGINAL_FILENAME ".dll"
            VALUE "ProductName",       "OmicronTK+Qt"
            VALUE "ProductVersion",    OTKQT_PRODUCT_VERSION_STR
        END
    END
    BLOCK "VarFileInfo"
    BEGIN
        VALUE "Translation", 0x0416, 1200
    END
END
